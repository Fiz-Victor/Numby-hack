{
  "schemaVersion": 1,
  "id": "numbyhacks",
  "version": "${version}",
  "name": "Numby Hack",
  "description": "A simple Meteor addon focused on giving users more customization options. Made in honor of Number81.",
  "authors": [
    "cqb13",
    "Crosby"
  ],
  "contact": {
    "repo": "https://github.com/cqb13/Numby-hack"
  },
  "icon": "assets/numby-hack/textures/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "cqb13.NumbyHack.NumbyHack"
    ]
  },
  "mixins": [
    "NumbyHack.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "146,188,98"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19.2",
    "meteor-client": ">=0.5.0"
  }
}
